    "# fabonacci\n",
    "def fn(n):\n",
    "    if n == 1:\n",
    "        return 1\n",
    "    if n == 2:\n",
    "        return 1\n",
    "    return fn(n-1) + fn(n-2)\n",
    "\n",
    "for i in range(1, 30):\n",
    "    print(i, fn(i))\n"
   ]
